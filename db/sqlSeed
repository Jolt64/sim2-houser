CREATE TABLE houses (
    id serial PRIMARY KEY,
    name VARCHAR(30),
    address VARCHAR(100),
    city VARCHAR(100),
    state VARCHAR(2),
    zip int,
    img TEXT,
    mortgage DECIMAL,
    rent DECIMAL
);


